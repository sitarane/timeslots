<h1>Sign Up</h1>
<%= form_with model: @user, url: users_path do |f| %>
  <div>
  <%= f.label 'name' %><br>
  <%= f.text_field :name %>
  </div>
  <div>
  <%= f.label 'email' %><br>
  <%= f.text_field :email %>
  </div>
  <div>
  <%= f.label 'password' %><br>
  <%= f.password_field :password %>
  </div>
  <div>
  <%= f.label 'password_confirmation' %><br>
  <%= f.password_field :password_confirmation %>
  </div>
  <div>
    <%= link_to "I already have an account", new_sessions_path %>
  <div>
  <div>
  <%= f.submit 'Sign Up' %>
  </div>
<% end %>
